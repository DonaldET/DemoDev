<?xml version="1.0" encoding="UTF-8"?>
<!-- Deployed version of application context -->
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<description>Text based configuration generator.</description>

	<import
		resource="classpath:/META-INF/main/spring/app-context.xml" />

	<bean id="generator"
		class="don.demo.generator.wrappers.TextSourceGeneratorImpl"
		scope="singleton">
	</bean>

</beans>
